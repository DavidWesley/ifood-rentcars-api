{
  "name": "@repo/server",
  "version": "0.0.2",
  "description": "API de aluguel de carros",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node --import tsx dist/main.js",
    "dev": "tsx watch src/main.ts",
    "build": "tsup --config tsup.config.ts src",
    "lint": "biome lint ./src --diagnostic-level=error",
    "format": "biome format ./src --write --diagnostic-level=warn"
  },
  "devDependencies": {
    "@repo/tsconfig": "*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@repo/drizzle": "*",
    "@repo/env": "*",
    "@utils-fns/validators": "^10.0.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "zod": "^3.22.4"
  }
}
