{
  "name": "ifood-rentcars-api",
  "version": "0.0.2",
  "description": "API de aluguel de carros do ifood",
  "private": "true",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "workspaces": [
    "src/*",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "NODE_ENV=production node --env-file=.env dist/main.js",
    "dev": "cross-env PORT=3333 tsx watch --env-file=.env src/main.ts",
    "build": "tsup --config tsup.config.ts src",
    "lint": "biome lint ./src --diagnostic-level=error",
    "lint:fix": "biome check --apply . --diagnostic-level=error",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.9"
  },
  "author": {
    "name": "David Wesley",
    "email": "contato.dwesley@outlook.com",
    "url": "https://github.com/DavidWesley"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidWesley/ifood-rentcars-api.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/DavidWesley/ifood-rentcars-api/issues"
  },
  "homepage": "https://github.com/DavidWesley/ifood-rentcars-api#readme",
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@tsconfig/node-lts": "^20.1.1",
    "@tsconfig/strictest": "^2.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "cross-env": "^7.0.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@repo/drizzle": "workspace:*",
    "@utils-fns/validators": "^10.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "zod": "^3.22.4"
  }
}
